[Unit]
Description=Ar4k Agent 
After=rc-local.service
[Service]
Type=simple
User=root
ExecStart=java -jar /opt/rossonet/ar4k-agent.jar --spring.shell.interactive.enabled=false
PIDFile=/opt/rossonet/ar4k/agent.pid
RemainAfterExit=no
Restart=on-failure
RestartSec=5s
[Install]
WantedBy=multi-user.target
